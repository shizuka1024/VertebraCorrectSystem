<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.vcsystem.ui.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_realtime"
        android:name="com.example.vcsystem.ui.RealtimeFragment"
        android:label="realtime"
        tools:layout="@layout/fragment_realtime" />

    <fragment
        android:id="@+id/navigation_reserve"
        android:name="com.example.vcsystem.ui.ReserveFragment"
        android:label="reserve"
        tools:layout="@layout/fragment_reserve" />

    <fragment
        android:id="@+id/navigation_chart"
        android:name="com.example.vcsystem.ui.ChartFragment"
        android:label="chart"
        tools:layout="@layout/fragment_chart" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.vcsystem.ui.UserFragment"
        android:label="user"
        tools:layout="@layout/fragment_user" />

</navigation>